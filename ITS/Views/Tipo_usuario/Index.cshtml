@model IEnumerable<ITS.Models.Tipo_usuario>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombre_tipo_usuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombre_tipo_usuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_tipo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_tipo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_tipo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
